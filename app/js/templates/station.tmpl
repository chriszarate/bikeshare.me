<p class="station <%= color %> <%= flags.station %>">
  <span class="title"><%= title %></span>
  <span class="status"><%= status %></span>
  <span class="docks <%= flags.docks || 'new' %>"><%= available.docks %> ○</span>
  <span class="bikes <%= flags.bikes || 'new' %>"><%= available.bikes %> ●</span>
</p>
